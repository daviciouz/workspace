package project2017;

public class ifstat1 {
	public static void main(String args[]){
		int test = 7;
		int test2 = 10;
		
		int bage = 23;
		int gage = 21;
		
		if(test==9){
			System.out.println("test is equal to 9");
		}
		else
		{
			System.out.println("test is not equal to 9");
		}
// OR being used here only one of the conditions must be true
		
		if(test>9 || test2==11){
			System.out.println("test is within range");
		}
		else
		{
			System.out.println("test is out of range");
		}
		
		System.out.println("This is a dating site for over 18+");
		
		if(bage>18 && gage>20){
			System.out.println("You can enter site");
		}
		else{
			System.out.println("You are cannot enter site");
		}
		
	}

}
