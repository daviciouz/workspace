package project2017;

public class mathoperation2 {
	public static void main(String args[]){
		int low = 10;
		int high = 8;
		
		low += 10;
		high +=8;
		
		System.out.println(low);
		System.out.println(high);
		
		low -= 10;
		high -=8;
		
		System.out.println(low);
		System.out.println(high);
	}

}
